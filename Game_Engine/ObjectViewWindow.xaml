        xmlns:local="clr-namespace:Game_Engine"
        xmlns:User_conts="clr-namespace:Game_Engine.User_controls"
        mc:Ignorable="d"
        Title="ObjectViewWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25*"/>
            <ColumnDefinition Width="75*"/>


        </Grid.ColumnDefinitions>

        <User_conts:Object_Items_List Grid.Column="0" x:Name="Components_list"/>
    </Grid>
</Window>
