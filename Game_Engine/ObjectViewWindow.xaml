<Window x:Class="Game_Engine.ObjectViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game_Engine"
        xmlns:User_conts="clr-namespace:Game_Engine.User_controls"
        mc:Ignorable="d"
        Title="ObjectViewWindow" Height="450" Width="800" KeyDown="Key_pressed">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25*"/>
            <ColumnDefinition Width="75*"/>


        </Grid.ColumnDefinitions>

        <User_conts:Object_Items_List Grid.Column="0" x:Name="Components_list" Panel.ZIndex="0" Background="Transparent"/>
        <Canvas x:Name="Object_display" Grid.Column="1" AllowDrop="True" Drop="Object_display_Drop" DragOver="Object_display_DragOver" Background="Transparent" Panel.ZIndex="2"/>
    </Grid>
</Window>
