<Window x:Class="Game_Engine.Project_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game_Engine"
        
        mc:Ignorable="d"
        Title="Project_Window" Height="450" Width="200"
        >
    <Grid >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"/>



        </Grid.ColumnDefinitions>

        <Canvas Name="Solution_explorer" Grid.Column="0">

            <TreeView Name="sol_exp_tree" Canvas.Top="20">
            </TreeView>
            <Button Content="reload" Click="Reload_Project_sol" />
        </Canvas>


    </Grid>
</Window>
